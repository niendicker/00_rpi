#In order to read and process any data from a slave modbus device the data bellow are mandatory.
#register+++ is the open TAG for a register data block
#space or comment after register block is the end tag

# Template
# MB_REGISTER_START
# function = modbus function code
# address  = register address
# size     = register size
# format   = US_08
# signal   = 0
# scale    = 256
# unit     = V
# meaning  = Generator_Volts
# lastValid = 0.0

#--------------------------------------
# GENERATOR VALUES
#--------------------------------------
#Generator frequency
MB_REGISTER_START
function = 04
address  = 00029
size     = 1
format   = US_08
signal   = 0
scale    = 256
unit     = Hz
meaning  = Generator_Frequency
lastValid = 0.0

#Generator average voltage
MB_REGISTER_START
function = 04
address  = 08005
size     = 2
format   = UL_08
signal   = 0
scale    = 256
unit     = V
meaning  = Generator_Avg_Voltage
lastValid = 0.0

#Generator apparent power
MB_REGISTER_START
function = 04
address  = 00065
size     = 2
format   = UL_08
signal   = 0
scale    = 256
unit     = KVA
meaning  = Generator_Apparent_Power
lastValid = 0.0

#Generator average active power
MB_REGISTER_START
function = 04
address  = 00061
size     = 2
format   = SL_08
signal   = 1
scale    = 256
unit     = KW
meaning  = Generator_Avg_Active_Power
lastValid = 0.0

#Generator average reactive power
MB_REGISTER_START
function = 04
address  = 00063
size     = 2
format   = SL_08
signal   = 1
scale    = 256
unit     = KVAr
meaning  = Generator_Avg_Reactive_Power
lastValid = 0.0

